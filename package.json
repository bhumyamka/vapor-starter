{
  "name": "vapor-starter",
  "version": "1.0.0",
  "description": "Starter kit for React & Redux",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/setup.test.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Bhumyamka",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "material-design-lite": "^1.3.0",
    "react": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.2",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-watch": "^3.1.5",
    "eventsource-polyfill": "0.9.6",
    "expect": "^23.1.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.6.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^1.13.0",
